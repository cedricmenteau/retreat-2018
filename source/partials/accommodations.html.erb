  <!-- Riad - Chez Darma -->
  <div class="riad-left">
    <div class="pic grid-picture">
      <a href="<%= image_path 'chez-darma-2.jpg' %>" class="progressive replace">
        <img src="" class="preview" />
      </a>
    </div>
