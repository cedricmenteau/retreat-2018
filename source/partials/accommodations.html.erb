      <h3>
        <span>Riad<br><i>Chez DarMa</i></span>
      </h3>
      <address>
       <%= link_to '234 rue Chbanate | Medina', 'https://goo.gl/maps/vNjVbqmjxgM2' %>, Essaouira 44000, Morocco
      </address>
    </div>
    <div class="pic-xs-2 grid-picture">
